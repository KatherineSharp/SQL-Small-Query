select	DISTINCT 
	C.BUSINESS_NO [ABN],
			DB.DEBTOR_ID,
			SD.DEBTOR_NAME,
		--SD.MAKE_KEY,
			--db.is_cash_sales_account,
			--TE.TEK,	
	PS.DESCRIPTION [SALES_GROUP]
	--SD.MAKE_DESCRIPTION [MAKE]
--SD.INVOICE_NO
	

from		CUSTOMER C
left JOIN DEBTOR DB ON DB.TEK_KEY = C.TEK_KEY
JOIN VEH_SALES SD ON SD.DEBTOR_ID = DB.DEBTOR_ID
JOIN VEH_SG P PS ON PS.VEH_SSG_ID = DB.VEH_SG_ID



WHERE (DB.SECT <> 102)
 --AND SD.MAKE_KEY = 20
 AND (C.ORG <> 7)
 		AND (C.EMAIL LIKE '%@%'	AND C.EMAIL LIKE '%.%'		and C.EMAIL not like '%noemail%'	)
		AND	C.PRIVACY IN(7,3,2)
ORDER BY 8

